{
	"manifest_version": 2,

	"name": "30min break saver",
	"short_name": "30breaksaver",
	"description": "How much time for 30 minutes break?",
	"version": "0.4",

	"page_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["js/backgroundScript.js"],
		"persistent": false
	  },
	"permissions": [ "declarativeContent", "activeTab" ]

}
